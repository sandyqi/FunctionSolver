<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.functionsolver"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/i1"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.functionsolver.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
           <activity
            android:name="com.example.functionsolver.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.functionsolver.MainActivity" />
 <category android:name="android.intent.category.DEFAULT"  />
                
            </intent-filter>
        </activity>
        
           <activity
            android:name="com.example.functionsolver.Listview"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.functionsolver.Listview" />
 <category android:name="android.intent.category.DEFAULT"  />
                
            </intent-filter>
        </activity>
    </application>

</manifest>
